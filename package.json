{
  "name": "pixi-inspector",
  "version": "0.8.0",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server",
    "test": "npm run lint",
    "lint": "eslint --ext js,vue src",
    "clean": "rm -rf build",
    "build": "webpack -p --progress",
    "build:clean": "npm run clean && webpack -p",
    "build:watch": "webpack -w --display none"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "vue",
    "plugins": [
      "only-warn"
    ],
    "globals": {
      "chrome": true
    }
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-vue-jsx"
    ]
  },
  "dependencies": {
    "@types/chrome": "^0.0.47",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.4.3",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "node-sass": "^4.5.3",
    "pixi-spine": "^1.5.3",
    "pixi.js": "^4.5.3",
    "rxjs": "^5.4.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue": "^2.4.2",
    "vue-loader": "^12.2.2",
    "vue-rx": "^3.2.0",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  }
}
